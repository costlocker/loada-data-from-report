{
  "name": "@cl/load-data-from-report",
  "version": "0.0.1",
  "description": "TypeScript project with GraphQL codegen for Costlocker GraphQL API",
  "scripts": {
    "start": "ts-node --transpile-only src/index.ts",
    "generate": "graphql-codegen --config codegen.ts",
    "precommit": "npm run format",
    "format": "prettier --write \"**/*.{ts,json}\""
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "cross-fetch": "^3.1.5",
    "dotenv": "^16.4.5",
    "graphql": "^16.10.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
